{
  "expo": {
    "name": "MindCare AI",
    "slug": "mindcare",
    "scheme": "mindcareai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/logo_mindcare.png",
    "splash": {
      "image": "./assets/images/logo_mindcare.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindcareai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_mindcare.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mindcareai.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO"
      ],
      "newArchEnabled": false
    },
    "web": {
      "favicon": "./assets/images/logo_mindcare.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-document-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "275fb931-7367-4329-a7c2-03cb67d6bd48"
      }
    },
    "owner": "yasserbhj"
  }
}
