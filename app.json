{
  "expo": {
    "name": "MindCare",
    "slug": "mindcare",
    "version": "1.0.0",
    "icon": "./assets/images/logo_mindcare.png",
    "splash": {
      "image": "./assets/images/logo_mindcare.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_mindcare.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/logo_mindcare.png"
    },
    "fonts": [
      {
        "asset": "node_modules/react-native-vector-icons/Fonts/FontAwesome.ttf",
        "fontFamily": "FontAwesome"
      }
    ]
  }
}